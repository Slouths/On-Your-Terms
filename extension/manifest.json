{
  "manifest_version": 3,
  "name": "On-you-terms",
  "version": "1.0",
  "description": "Simplify and summarize terms of service",
  "permissions": [
    "activeTab", "tabs"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  }
}